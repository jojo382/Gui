-- Scripts:

local function BPVEYS_fake_script() -- AutoFarm.LocalScript 
	local script = Instance.new('LocalScript', AutoFarm)

	script.Parent.MouseButton1Click:connect(function()
		script.Parent.Parent.page.autofarm.Visible = true
		script.Parent.Parent.page.teleport.Visible = false
		script.Parent.Parent.page.Misc.Visible = false
		script.Parent.Parent.page.devilfruit.Visible = false
		script.Parent.Parent.page.stats.Visible = false
		script.Parent.Parent.page.Team.Visible = false
		script.Parent.Parent.page.Setting.Visible = false
		script.Parent.Parent.page.dev.Visible = false
		wait(0.1)
	end)
end
coroutine.wrap(BPVEYS_fake_script)()
